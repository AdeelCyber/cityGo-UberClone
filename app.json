{
  "expo": {
    "name": "Q Drive",
    "slug": "citygo-dev",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/notificationIcon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to display on your profile."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/notificationIcon.png",
          "color": "#ffffff"
        }
      ],
      "expo-font",
      "expo-secure-store"
    ],
    "splash": {
      "image": "./assets/logo_sp1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "icon": "./assets/notificationIcon.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.lunatech.CityGo",
      "config": {
        "googleMapsApiKey": "AIzaSyBSXECJBiw1cTjkx0kxV_1ReIlV1DlRVL8"
      }
    },
    "android": {
      "icon": "./assets/notificationIcon.png",
      "package": "com.lunatech.CityGo",
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBSXECJBiw1cTjkx0kxV_1ReIlV1DlRVL8"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "b24e508c-b0cc-4b0e-8073-9ac500ef68ed"
      }
    }
  }
}
